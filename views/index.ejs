<% layout('/layouts/boilerplate') %>
    <h1>All Campgrounds:</h1>
    <div>
        <a href="/campground/new/">
            <h2>New Campground</h2>
        </a>
    </div>
    <ul>
        <% for( let campground of campgrounds ) { %>
            <li>
                <a href="/campground/<%= campground._id %>">
                    <%= campground.title %>
                </a>
            </li>
            <% } %>
    </ul>